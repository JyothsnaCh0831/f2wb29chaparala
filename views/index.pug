extends layout

block content
  h1= title
  p Assignment 10
  p Welcome to #{title}
  button(type="button" class="btn btn-success" onclick="doBump()") Press Me
  br
  br
  div#bump Bump the count

  script.
    var count = 0
    var bump = 1
    function doBump() {
      count += bump
      bump += 1
      document.getElementById("bump").innerHTML = "Count " + count + " Bump " + bump;
    }
    